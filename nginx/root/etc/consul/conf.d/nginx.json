{
    "service": {
        "name": "nginx",
        "port": 80,
        "checks": [{
            "id": "api",
            "name": "nginx on port 80",
            "http": "http://localhost:80/ping",
            "interval": "10s",
            "timeout": "1s"
        }, {
            "id": "websocket-server",
            "name": "nginx on port 8000",
            "http": "http://localhost:8000/ping",
            "interval": "10s",
            "timeout": "1s"
        }]
    }
}
